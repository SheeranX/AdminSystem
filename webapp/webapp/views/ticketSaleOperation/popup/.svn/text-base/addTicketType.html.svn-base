    <script src="../../../common/tools/layui/layui.all.js"></script>
    <script>
        var laydate = layui.laydate;
        laydate.render({
            elem: '#startDate' //指定元素
        });
        laydate.render({
            elem: '#endDate' //指定元素
        });
    </script>
